generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(uuid())
  studentCode String
  email       String @unique
  name        String
  school      String

  @@unique([school, studentCode])
}
